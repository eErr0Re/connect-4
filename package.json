{
  "name": "connect-4",
  "version": "1.0.0",
  "description": "Connect4 web game.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "redirect": "redirect() { sudo iptables -t nat -I PREROUTING -p tcp --dport ${1} -j REDIRECT --to-ports ${2} && sudo iptables -t nat -I OUTPUT -p tcp -o lo --dport ${1} -j REDIRECT --to-ports ${2} && echo \"Redirected port ${1} to port ${2}\" && node app.js ${2}; }; redirect",
